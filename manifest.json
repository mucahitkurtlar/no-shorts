{
    "name": "No Shorts",
    "description": "No more YouTube Shorts!",
    "version": "2.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_start"
        }
    ]
}